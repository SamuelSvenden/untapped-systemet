{
    "manifest_version": 3,
    "name": "Systembolaget Untappd Searcher",
    "version": "1.0",
    "description": "Searches Untappd for beers on the systembolaget page",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "https://www.systembolaget.se/*"
    ],
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  